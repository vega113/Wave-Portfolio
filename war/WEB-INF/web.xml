<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

	<!-- Servlets -->
	<servlet>
		<servlet-name>remoteLoggerServiceImpl</servlet-name>
		<servlet-class>com.allen_sauer.gwt.log.server.RemoteLoggerServiceImpl</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>gFinanceServlet</servlet-name>
		<servlet-class>com.aggfi.portfolio.wave.server.GFinanceServiceImpl</servlet-class>
	</servlet>


	<servlet-mapping>
		<servlet-name>remoteLoggerServiceImpl</servlet-name>
		<url-pattern>/waveportfolio/gwt-log</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>gFinanceServlet</servlet-name>
		<url-pattern>/waveportfolio/gfinance</url-pattern>
	</servlet-mapping>
	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>WavePortfolio.html</welcome-file>
	</welcome-file-list>

</web-app>
